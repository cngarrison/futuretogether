{
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "FT_STAFF_PASSWORD_HASH=$(cat .staff-pass-hash) FT_TURNSTILE_SITE_KEY='1x00000000000000000000AA' FT_TURNSTILE_SECRET_KEY='1x0000000000000000000000000000000AA' vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "unstable": ["kv"],
  "exclude": [
    "**/_fresh/*"
  ],
  "include": [
    "events",
    "blog"
  ],
  "imports": {
    "@/": "./",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "preact": "npm:preact@^10.28.3",
    "preact/hooks": "npm:preact@10.28.3/hooks",
    "@preact/signals": "npm:@preact/signals@^2.5.0",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.16",

    "marked": "npm:marked@17.0.2",
    "marked-mangle": "npm:marked-mangle@1.1.12",
    "marked-gfm-heading-id": "npm:marked-gfm-heading-id@4.1.3",
    "marked-smartypants": "npm:marked-smartypants@1.1.11",

    "ical-generator": "npm:ical-generator@^10.0.0",
    "@touch4it/ical-timezones": "npm:@touch4it/ical-timezones@^1.9.0",
    "luxon": "npm:luxon@^3.7.2",

    "@std/assert": "jsr:@std/assert@^1.0.12",
    "@std/async": "jsr:@std/async@^1.0.12",
    "@std/cli": "jsr:@std/cli@^1.0.16",
    "@std/csv": "jsr:@std/csv@^1.0.6",
    "@std/crypto": "jsr:@std/crypto@^1.0.4",
    "@std/datetime": "jsr:@std/datetime@^0.225.4",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/encoding": "jsr:@std/encoding@^1.0.9",
    "@std/fmt": "jsr:@std/fmt@^1.0.6",
    "@std/fmt/colors": "jsr:@std/fmt@^1.0.6/colors",
    "@std/fs": "jsr:@std/fs@^1.0.16",
    "@std/html": "jsr:@std/html@^1.0.3",
    "@std/http": "jsr:@std/http@^1.0.14",
    "@std/io": "jsr:@std/io@^0.225.2",
    "@std/log": "jsr:@std/log@^0.224.14",
    "@std/media-types": "jsr:@std/media-types@^1.1.0",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/regex": "jsr:@std/regex@^1.0.1",
    "@std/semver": "jsr:@std/semver@^1.0.5",
    "@std/streams": "jsr:@std/streams@^1.0.9",
    "@std/testing/bdd": "jsr:@std/testing@^1.0.11/bdd",
    "@std/testing/mock": "jsr:@std/testing@^1.0.11/mock",
    "@std/text": "jsr:@std/text@^1.0.16",
    "@std/toml": "jsr:@std/toml@^1.0.8",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "@std/uuid": "jsr:@std/uuid@^1.0.6",
    "@std/yaml": "jsr:@std/yaml@^1.0.5"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
